{
  "name": "<%= props.name %>",
  "description": "",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run serve",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:beta": "better-npm-run build:beta",
    "build:prod": "better-npm-run build:prod",
    "serve": "better-npm-run dll && better-npm-run serve",
    "serve:normal": "better-npm-run dll && better-npm-run serve:normal",
    "serve:dist": "better-npm-run build && better-npm-run serve:dist",
    "eslint": "better-npm-run eslint",
    "dll": "better-npm-run dll"
  },
  "betterScripts": {
    "dll": {
      "command": "packing dll"
    },
    "serve": {
      "command": "webpack-dashboard --title <%= props.name %> -- packing serve"
    },
    "serve:normal": {
      "command": "packing serve"
    },
    "serve:dist": {
      "command": "packing serve:dist"
    },
    "build": {
      "command": "packing build"
    },
    "build:dev": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "development",
        "CDN_ROOT": "//l-wap1.wap.dev.cn6.qunar.com/xxx/dev/assets"
      }
    },
    "build:beta": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "beta",
        "CDN_ROOT": "//mobileqzz.beta.qunar.com/xxx/prd/assets"
      }
    },
    "build:prod": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "production",
        "CDN_ROOT": "//q.qunarzz.com/xxx/prd/assets"
      }
    },
    "eslint": {
      "command": "./node_modules/.bin/eslint src"
    }
  },
  "dependencies": {<% if (props.css === 'less') { %>
    "less": "^2.7.1",
    "less-loader": "^2.2.3",<% } if (props.css === 'sass') { %>
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",<% } if (props.react) { %>
    "babel-plugin-react-transform": "^2.0.2",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "react": "^15.3.1",
    "react-a11y": "^0.3.3",
    "react-dom": "^15.3.1",
    "react-transform-catch-errors": "^1.0.2",<% } if (props.redux) { %>
    "react-redux": "^4.4.5",
    "redbox-react": "^1.3.0",
    "redux": "^3.5.2",<% } %>
    "packing": "^<%= props.packingVersion %>"
  },
  "devDependencies": {
    "express": "^4.14.0",
    "open-browser-webpack-plugin": "^0.0.3",<% if (props.template === 'artTemplate') { %>
    "packing-template-artTemplate": "^1.0.1",<% } if (props.template === 'ejs') { %>
    "packing-template-ejs": "^1.0.0",<% } if (props.template === 'handlebars') { %>
    "packing-template-handlebars": "^1.0.0",<% } if (props.template === 'html') { %>
    "packing-template-html": "^1.0.0",<% } if (props.template === 'pug') { %>
    "packing-template-pug": "^1.0.5",<% } if (props.template === 'smarty') { %>
    "packing-template-smarty": "^1.0.3",<% } if (props.template === 'velocity') { %>
    "packing-template-velocity": "^1.0.2",<% } %>
    "packing-urlrewrite": "^0.1.7",<% if (props.redux) { %>
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",<% } %>
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "engines": {
    "node": ">=0.12.7",
    "npm": ">=3.0"
  }
}
