{
  "name": "library",
  "description": "",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run serve",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:beta": "better-npm-run build:beta",
    "build:prod": "better-npm-run build:prod",
    "serve": "better-npm-run serve",
    "serve:normal": "better-npm-run serve:normal",
    "serve:dist": "better-npm-run build && better-npm-run serve:dist",
    "eslint": "better-npm-run eslint"
  },
  "betterScripts": {
    "serve": {
      "command": "webpack-dashboard --title api -- packing serve"
    },
    "serve:normal": {
      "command": "packing serve"
    },
    "serve:dist": {
      "command": "packing serve:dist"
    },
    "build": {
      "command": "packing build"
    },
    "build:dev": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:beta": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "beta"
      }
    },
    "build:prod": {
      "command": "better-npm-run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "eslint": {
      "command": "eslint src"
    }
  },
  "pre-commit": [
    "eslint"
  ],
  "dependencies": {
    "packing": "../.."
  },
  "devDependencies": {
    "express": "^4.14.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "packing-template-html": "^1.0.0",
    "packing-urlrewrite": "^0.1.7",
    "pre-commit": "^1.2.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  },
  "engines": {
    "node": ">=4.0",
    "npm": ">=3.0"
  }
}
